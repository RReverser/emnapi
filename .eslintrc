{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "plugins": ["import"],
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "HEAPU8": false,
        "HEAPU32": false,
        "HEAPF64": false,
        "UTF8ToString": false,
        "Pointer_stringify": false,
        "FUNCTION_TABLE_iii": false,
        "FUNCTION_TABLE_viiii": false,
        "global": false,
        "self": false,
        "Symbol": false,
        "ArrayBuffer": false,
        "Module": false,
        "module": false
    }
}